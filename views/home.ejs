<%- include("partials/header") %>
<section id=title>
  <div class="container-fluid">
    <nav class="navbar navbar-expand-lg navbar-light ">
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <a class="navbar-brand" href="">Journal</a>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav ms-auto">
          <li class="nav-item"><a class="nav-link" href="#feature">Features</a></li>
          <li class="nav-item"><a class="nav-link" href="#footer-section">About</a></li>
        </ul>
      </div>
    </nav>
    <div class="row">
      <div class="col-lg-6">
        <h1>Write your thoughts away!</h1>
        <button class="btn btn-lg btn-dark button-sign"><a href="/register" class="text-light">Sign up</a></button>
        <button class="btn btn-lg btn-dark button-sign"><a href="/login" class="text-light">Sign In</a></button>

      </div>
      <div class="col-lg-6">
        <img class="title-img"
          src="data:image/jpeg;base64,"
          alt="Journal-img">
      </div>
    </div>
  </div>
</section>

<section id="feature">
  <div class="row">
      <div class="feature-box col-lg-6">
        <i class="fa-solid fa-a fa-4x"></i>
        <h3>Easy</h3>
        <p>Just 280 characters</p>
      </div>

      <div class="feature-box col-lg-6">
        <i class="fa-regular fa-clock fa-4x"></i>
        <h3>Quick</h3>
        <p>Journal for set amount of time</p>
      </div>
    </div>
</section>
<%-include("partials/footer") %>









